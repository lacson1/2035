{
  "name": "physician-dashboard-2035",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "deploy": "./scripts/auto-deploy.sh",
    "setup-env": "./scripts/setup-env-vars.sh",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:watch": "tsc --watch & vite build --watch",
    "build:analyze": "npm run build && npx vite-bundle-visualizer",
    "build:check": "tsc --noEmit && npm run build",
    "build:monitor": "./scripts/monitor-build.sh",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:accessibility": "vitest --run src/components/__tests__/*.accessibility.test.*",
    "test:all": "npm run test && npm run test:accessibility && npm run test:e2e",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@sentry/react": "^7.91.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^3.3.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.9.1",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "isomorphic-dompurify": "^2.1.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^1.0.4",
    "vitest-axe": "^0.1.0",
    "workbox-core": "^7.3.0",
    "workbox-window": "^7.3.0",
    "zod": "^3.22.4"
  }
}
